    "gulp-eslint": "^2.0.0",
    "gulp-header": "^1.7.1",
    "gulp-jsbeautifier": "^2.0.3",
    "gulp-preprocess": "^2.0.0",
    "gulp-sequence": "^0.4.5",
    "gulp-uglify": "^1.5.3",
    "jaguarjs-jsdoc": "^1.0.0",
